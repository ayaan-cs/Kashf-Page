{
  "version": 8,
  "name": "Kashf Teal Gold Dark",
  "metadata": {
    "mapbox:autocomposite": true,
    "mapbox:type": "template",
    "mapbox:groups": {}
  },
  "center": [-97.7089, 30.4583],
  "zoom": 11,
  "bearing": 0,
  "pitch": 0,
  "sources": {
    "composite": {
      "url": "mapbox://mapbox.mapbox-streets-v8,mapbox.mapbox-terrain-v2",
      "type": "vector"
    }
  },
  "sprite": "mapbox://sprites/mapbox/dark-v11",
  "glyphs": "mapbox://fonts/mapbox/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "#0A0A0A"
      }
    },
    {
      "id": "landcover",
      "type": "fill",
      "source": "composite",
      "source-layer": "landcover",
      "maxzoom": 12,
      "paint": {
        "fill-color": "#1A1A1A",
        "fill-opacity": 0.5
      }
    },
    {
      "id": "water",
      "type": "fill",
      "source": "composite",
      "source-layer": "water",
      "paint": {
        "fill-color": "#14B8A6",
        "fill-opacity": 0.6
      }
    },
    {
      "id": "waterway",
      "type": "line",
      "source": "composite",
      "source-layer": "waterway",
      "paint": {
        "line-color": "#14B8A6",
        "line-width": 1,
        "line-opacity": 0.5
      }
    },
    {
      "id": "land",
      "type": "background",
      "paint": {
        "background-color": "#1A1A1A"
      }
    },
    {
      "id": "landuse",
      "type": "fill",
      "source": "composite",
      "source-layer": "landuse",
      "paint": {
        "fill-color": [
          "match",
          ["get", "class"],
          "park",
          "#2A2A2A",
          "airport",
          "#252525",
          "cemetery",
          "#222222",
          "#1A1A1A"
        ],
        "fill-opacity": 0.8
      }
    },
    {
      "id": "national_park",
      "type": "fill",
      "source": "composite",
      "source-layer": "landuse_overlay",
      "filter": ["==", "class", "national_park"],
      "paint": {
        "fill-color": "#2A2A2A",
        "fill-opacity": 0.5
      }
    },
    {
      "id": "building",
      "type": "fill",
      "source": "composite",
      "source-layer": "building",
      "minzoom": 15,
      "paint": {
        "fill-color": "#333333",
        "fill-opacity": 0.8
      }
    },
    {
      "id": "building-3d",
      "type": "fill-extrusion",
      "source": "composite",
      "source-layer": "building",
      "minzoom": 15,
      "filter": ["has", "height"],
      "paint": {
        "fill-extrusion-color": "#4A4A4A",
        "fill-extrusion-height": ["get", "height"],
        "fill-extrusion-base": ["get", "min_height"],
        "fill-extrusion-opacity": 0.9
      }
    },
    {
      "id": "tunnel-street-minor",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "filter": ["all", ["==", "structure", "tunnel"], ["in", "class", "street", "street_limited"]],
      "paint": {
        "line-color": "#333333",
        "line-width": 2,
        "line-opacity": 0.5,
        "line-dasharray": [3, 1]
      }
    },
    {
      "id": "road-street",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "filter": ["in", "class", "street", "street_limited"],
      "paint": {
        "line-color": "#4A4A4A",
        "line-width": [
          "interpolate",
          ["linear"],
          ["zoom"],
          12, 1,
          18, 6
        ]
      }
    },
    {
      "id": "road-primary",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "filter": ["==", "class", "primary"],
      "paint": {
        "line-color": "#666666",
        "line-width": [
          "interpolate",
          ["linear"],
          ["zoom"],
          10, 1,
          18, 8
        ]
      }
    },
    {
      "id": "road-secondary-tertiary",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "filter": ["in", "class", "secondary", "tertiary"],
      "paint": {
        "line-color": "#5A5A5A",
        "line-width": [
          "interpolate",
          ["linear"],
          ["zoom"],
          10, 0.5,
          18, 6
        ]
      }
    },
    {
      "id": "road-motorway-trunk",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "filter": ["in", "class", "motorway", "trunk"],
      "paint": {
        "line-color": "#D4AF37",
        "line-width": [
          "interpolate",
          ["linear"],
          ["zoom"],
          8, 1,
          18, 12
        ],
        "line-opacity": 0.7
      }
    },
    {
      "id": "bridge-street",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "filter": ["all", ["==", "structure", "bridge"], ["in", "class", "street", "street_limited"]],
      "paint": {
        "line-color": "#5A5A5A",
        "line-width": 3
      }
    },
    {
      "id": "bridge-primary",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "filter": ["all", ["==", "structure", "bridge"], ["in", "class", "primary", "secondary", "tertiary"]],
      "paint": {
        "line-color": "#D4AF37",
        "line-width": 4,
        "line-opacity": 0.6
      }
    },
    {
      "id": "admin-state-province",
      "type": "line",
      "source": "composite",
      "source-layer": "admin",
      "filter": ["==", "admin_level", 1],
      "paint": {
        "line-color": "#14B8A6",
        "line-width": 1.5,
        "line-opacity": 0.4,
        "line-dasharray": [4, 2]
      }
    },
    {
      "id": "place-label-city",
      "type": "symbol",
      "source": "composite",
      "source-layer": "place_label",
      "filter": ["==", "type", "city"],
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["DIN Offc Pro Medium", "Arial Unicode MS Regular"],
        "text-size": 16
      },
      "paint": {
        "text-color": "#FFFFFF",
        "text-halo-color": "#0A0A0A",
        "text-halo-width": 2
      }
    },
    {
      "id": "place-label-town",
      "type": "symbol",
      "source": "composite",
      "source-layer": "place_label",
      "filter": ["==", "type", "town"],
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["DIN Offc Pro Regular", "Arial Unicode MS Regular"],
        "text-size": 12
      },
      "paint": {
        "text-color": "#CCCCCC",
        "text-halo-color": "#0A0A0A",
        "text-halo-width": 1.5
      }
    },
    {
      "id": "road-label-primary",
      "type": "symbol",
      "source": "composite",
      "source-layer": "road",
      "filter": ["in", "class", "primary", "secondary"],
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["DIN Offc Pro Regular", "Arial Unicode MS Regular"],
        "text-size": 10,
        "symbol-placement": "line"
      },
      "paint": {
        "text-color": "#D4AF37",
        "text-halo-color": "#0A0A0A",
        "text-halo-width": 2
      }
    },
    {
      "id": "poi-label",
      "type": "symbol",
      "source": "composite",
      "source-layer": "poi_label",
      "minzoom": 14,
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["DIN Offc Pro Regular", "Arial Unicode MS Regular"],
        "text-size": 10
      },
      "paint": {
        "text-color": "#999999",
        "text-halo-color": "#0A0A0A",
        "text-halo-width": 1
      }
    }
  ]
}
